{
  "extends": "airbnb-base",
  "rules": {
    "max-len": 1,
    "quotes": [1, "double"],
    "arrow-parens": 0,
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
    "no-param-reassign": 0,
    "no-plusplus": [1, { "allowForLoopAfterthoughts": true }],
    "consistent-return": [1, { "treatUndefinedAsUnspecified": true }]
  }
}
